    :label "Preferred label" ;
    rdfs:comment "The preferred label for a thing" ;
    :display false ;
    :editable true ;
    :shortName "label" .

qaat:label a owl:DatatypeProperty ;
    :label "Concept label" ;
    :display false ;
    :editable false ;
    rdfs:comment "A label for a concept from a controlled vocab" .
#    rdfs:comment "Temporary placeholder for property that should become a SKOS concept." .

rdfs:label a owl:DatatypeProperty ;
    :label "Label" ;
    :display true ;
    :editable true ;
    rdfs:comment "A label for a thing" ;
    :shortName "altlabel" .

rdf:type a owl:DatatypeProperty ;
    :label "Type" ;
