dcterms:created a owl:ObjectProperty ;
    :label "Created" ;
    :display false ;
    :editable false ;
    rdfs:isDefinedBy dcterms:created .

dcterms:creator a owl:ObjectProperty ;
    :label "Creator" ;
    :display true ;
    :editable false ;
    rdfs:isDefinedBy dcterms:creator .

dcterms:title a owl:DatatypeProperty ;
